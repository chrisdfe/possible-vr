        </a-entity>
      </a-entity>
      -->

      <!-- fireplace -->
      <a-entity
        position="3 0 8"
        scale="0.02 0.02 0.02"
        rotation="0 0 0"
        obj-model="obj: #fireplace-obj; mtl: #fireplace-mtl">
      </a-entity>

      <a-entity position="0 1.8 2">
        <a-entity camera look-controls wasd-controls></a-entity>
      </a-entity>
    </a-scene>

    <script src="/scripts/index.js"></script>
